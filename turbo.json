{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "outputs": [
          "build/**",
          "dist/**"
        ]
      },
      "test": {
        "cache": true
      },
      "test:coverage": {
        "outputs": ["coverage/**"],
        "cache": true
      },
      "lint": {
        "cache": true
      },
      "typecheck": {
        "cache": true
      }
    }
  }